snippet res "resource ... { ... }" b
resource "${1:resource_type}" "${2:resource_name}" {
	$0
}
endsnippet

snippet var "variable ... {}" b
variable "${1:var_name}" {}$0
endsnippet

snippet varb "variable ... { ... }" b
variable "${1:var_name}" {
	${2:default = "$3"}
}$0
endsnippet

snippet out "output ... { ... }" b
output "${1:output_name}" {
	value = "$2"
}$0
endsnippet
